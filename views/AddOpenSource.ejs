  
<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3">
        <i class="fas fa-user-plus"></i> Add new open-source
      </h1>
      <%- include('./partials/messages'); -%>
      <form action="/users/AddOpenSource" method="POST">
        <div class="form-group">
          <label for="email">Email</label>
          <input
            type="email"
            id="email"
            name="email"
            class="form-control"
            placeholder="Enter Email"
            value="<%= typeof email != 'undefined' ? email : '' %>"
          />
        </div>
        <div class="form-group">
          <label for="osURL">OS URL</label>
          <input
            type="text"
            id="osURL"
            name="osURL"
            class="form-control"
            placeholder="Enter osURL"
            value="<%= typeof name != 'undefined' ? name : '' %>"
          />
        </div>
        <button type="submit" class="btn btn-primary btn-block">
          Submit
        </button>
      </form>
  </div>
</div>